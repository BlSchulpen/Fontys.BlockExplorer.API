{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:61877",
      "sslPort": 44393
    }
  },
  "profiles": {
    "Fontys.BlockExplorer.API": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
